     if(typeof(liElem) != "undefined") {
         liElem.style.display = "list-item";
         var liElemWidth = liElem.offsetWidth;
         var liElemDefaultWidth = 187;
         liElemWidth = liElemWidth > 0 ? liElemWidth : liElemDefaultWidth;
         var numItemsToShow = Math.floor(ulElemWidth/liElemWidth);
         var shId = "asinSummaryShoveler-1S2ZKJ3PRA8MZMD37ZV0";
         var size = numItemsToShow > liElems.length ? liElems.length : numItemsToShow;
         var classes = [];
         for (var i=0; i < size; i++) 
             classes.push(liElems[i].className.match(/item[0-9]([0-9]|)/i)[0]);
         displayItems(shId,numItemsToShow,ulElemWidth,liElemWidth,classes);
     }
     </script>
 
</div>
