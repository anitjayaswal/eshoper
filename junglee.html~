<script>
(function() {
    if(document.getElementById('searchCategory') === null)
        return;
    var i,bars = document.querySelectorAll('.searchBarContainer');
    function hasCls(ele,cls) {
        return ele.className.match(new RegExp('(\\s|^)'+cls+'(\\s|$)'));
    }
    function addCls(ele,cls) {
        if(!hasCls(ele,cls)) ele.className += " "+cls;
    }
    function rmCls(ele,cls) {
        if(hasCls(ele,cls)) {
            var reg = new RegExp('(\\s|^)'+cls+'(\\s|$)');
            ele.className=ele.className.replace(reg,' ');
        }
    }
    function searchSync(searchBar) {
        function addEvent(el,ev,fn) {
            if (el.addEventListener) {
                el.addEventListener(ev, fn, false);
            } else if (el.attachEvent)  {
                el.attachEvent('on'+ev, fn);
            }
        }
        var label = searchBar.querySelector('.searchCategoryLabel'),
            text = searchBar.querySelector('.searchCategoryLabelText'),
            selector = searchBar.querySelector('.searchCategory'),
            clip = searchBar.querySelector('.searchCategoryClip'),
            container = searchBar.querySelector('.searchCategoryContainer');

        var sync = function(){
            text.innerHTML = selector.options[selector.selectedIndex].innerHTML;
            clip.style.width = (label.offsetWidth + 5) + 'px';
            container.style.width = (label.offsetWidth - 1) + 'px';
            setTimeout(function(){
                if(searchBar.querySelector('.searchCategory:active,.searchCategory:focus')) {
                    if(!hasCls(clip,'active')) {
                        addCls(clip,'active')
                    }
                } else {
                    if(hasCls(clip,'active')) {
                        rmCls(clip,'active')
                    }
                }
            }, 1);
        };
        addEvent(selector, 'change', function(e) {
            sync();
            setTimeout(function(){
                searchBar.querySelector('input.searchInput').focus();
            },1);
        });
        addEvent(selector, 'click', sync);
        addEvent(selector, 'blur', sync);
        addEvent(selector, 'focus', sync);
        addEvent(selector, 'keyup', sync);
        sync.call(selector);
    }
    for(i=0; i<bars.length; i++){
        searchSync(bars[i]);
    }
}());
</script>

