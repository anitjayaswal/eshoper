        <script type="text/javascript">
            






    if (window.navigator.appName == "Microsoft Internet Explorer" &&
       (document.documentMode === undefined || document.documentMode < 9)
    ) {
        (function() {
            if (document.documentMode == 8) {
                document.documentElement.className += " ie8";
            } else {
                var BROWSER_SUPPORT_PATH = "/browser-support";
                if (document.location.pathname != BROWSER_SUPPORT_PATH)
                    document.location = BROWSER_SUPPORT_PATH;
            }
        })();
    }




    function displayItems(id,numItemsToShow,totalWidth,itemWidth,classes) {
        var fixedPart = "#" + id + " ul li.",

            itemStyle = (numItemsToShow > 0 ? (fixedPart + classes.join(","+fixedPart)) : ''),
            remainingSpace = totalWidth - numItemsToShow*itemWidth,
            margin = Math.floor(remainingSpace/numItemsToShow/2);
        itemStyle += "{display: list-item!important;margin-left:" + margin + "px;margin-right:" + margin + "px;}";
        applyCSS(itemStyle);
        function applyCSS(customStyle) {
            var headNode = document.getElementsByTagName('head')[0],
                styleNode = document.createElement('style');
            styleNode.setAttribute('type','text/css');
            if (styleNode.styleSheet)
                styleNode.styleSheet.cssText = customStyle;
            else
                styleNode.appendChild(document.createTextNode(customStyle));
            headNode.appendChild(styleNode);
        }
    }



    if (window.amznJQ) {
        
            amznJQ._timesliceJS = false;
        
        amznJQ.available('jQuery', function() {
            if (!window.$)
                window.$ = jQuery;
        });
    }






  
    
    (function() {
        var events = { 
          
            
            'af': { numTriggers: 1, fired: false }
          
        };
        window.csmEvents = {
            init: function(event, numTriggers) {
                events[event] = {
                    numTriggers: numTriggers || 1,
                    fired: false
                };
            },
            trigger: function(event) {
                var m = events[event];
                if ((--m.numTriggers) > 0 || m.fired)
                    return;
                m.fired = true;
                if (typeof uet == 'function')
                    uet(event);
            },
            incr: function(event, numTriggers) {
                events[event].numTriggers += numTriggers || 1;
            }
        };
    })();

  
  
    (function() {
         csmEvents.incr('af') ; 
        var img = new Image();
        img.onload = function() {
             csmEvents.trigger('af') ;
            uex("ld", "af-sprite", {wb:1});
        };
        img.src = "http://ec2.images-amazon.com/images/G/31/junglee/sitewide/junglee_sprite_atf-02-02-2015._V1_.png";
    })();
    
  





